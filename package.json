{
	"name": "open-hexdoc",
	"repository": {
		"type": "git",
		"url": "https://github.com/w-sanches/vscode-open-hexdoc"
	},
	"publisher": "w-sanches",
	"displayName": "Open HexDoc",
	"description": "",
	"version": "0.0.5",
	"type": "module",
	"engines": {
		"node": ">=14.16",
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:./mix.lock"
	],
	"exports": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "open-hexdoc.openHexDoc",
				"title": "Open HexDoc"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"got": "^12.5.3"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "^18.0.0",
		"@types/vscode": "^1.68.0",
		"@typescript-eslint/eslint-plugin": "^5.28.0",
		"@typescript-eslint/parser": "^5.28.0",
		"eslint": "^8.17.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.3",
		"vscode-test": "^1.3.0"
	}
}
